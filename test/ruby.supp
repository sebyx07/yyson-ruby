   ruby_iseq
   Memcheck:Leak
   ...
   fun:*iseq*
}

{
   ruby_compile
   Memcheck:Leak
   ...
   fun:*compile*
}

{
   ruby_prism
   Memcheck:Leak
   ...
   fun:*prism*
}

{
   ruby_ibf
   Memcheck:Leak
   ...
   fun:*ibf*
}

{
   ruby_vm
   Memcheck:Leak
   ...
   fun:*vm*
}
